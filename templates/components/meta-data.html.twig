<div class="meta-data">
  <dl class="meta-data__list">
    {% for item in items %}
      <dt class="visually-hidden">{{ item.title }}</dt>
      <dd class="{{ item.classname ? item.classname : item.title|lower }}"{{ item.itemprop ? ' itemprop="'~ item.itemprop ~'"' }}>{{ item.descr }}</dd>
    {% endfor %}
  </dl>
</div>